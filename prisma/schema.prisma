generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = "5"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Promotion {
  id                  Int     @id @default(autoincrement())
  category1           String?
  category2           String?
  location            String?
  validity            String?
  discount            String?
  terms               String?
  condition           String?
  businessDescription String?
  misc1               String?
  merchant            String?
  address             String?
  direction           String?
  coordinates         String?
  phoneNumber         String?
  businessHours       String?
  closed              String?
  misc2               String?
  merchantWebsite     String?
  etc                 String?
  promotionUrl        String?
}
